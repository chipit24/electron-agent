{
  "name": "electron-agent",
  "productName": "electron-agent",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "format": "prettier . --write",
    "type-check": "npm run type-check:renderer && npm run type-check:node",
    "type-check:renderer": "tsc --noEmit --project tsconfig.renderer.json",
    "type-check:node": "tsc --noEmit --project tsconfig.node.json"
  },
  "keywords": [],
  "author": {
    "name": "Robert Komaromi",
    "email": "chipit24@users.noreply.github.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@tailwindcss/vite": "^4.1.10",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.5.2",
    "electron": "36.5.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "3.5.3",
    "tailwindcss": "^4.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.0",
    "vite": "^5.4.19"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "engines": {
    "node": "22",
    "npm": "11"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "22",
      "onFail": "warn"
    },
    "packageManager": {
      "name": "npm",
      "version": "11",
      "onFail": "warn"
    }
  }
}
